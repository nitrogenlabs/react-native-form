{
  "name": "@nlabs/react-native-form",
  "version": "0.4.5",
  "description": "React Native form components",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "button",
    "form",
    "input",
    "inputfield",
    "select",
    "selectfield",
    "datepicker",
    "datetime",
    "datetimefield",
    "tags",
    "tagfield",
    "textfield",
    "textinput",
    "nitrogenlabs",
    "react-native"
  ],
  "author": "Giraldo Rosales <giraldo@nitrogenlabs.com> (http://nitrogenlabs.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nitrogenlabs/react-native-form"
  },
  "homepage": "http://nitrogenlabs.com",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/react-native-form/issues"
  },
  "scripts": {
    "compile": "NODE_ENV=production tsc",
    "lint": "tslint -c ./tslint.json './src/**/*.{ts,tsx}'",
    "prepublishOnly": "NODE_ENV=production npm run lint && npm run compile",
    "pretest": "npm run lint",
    "reset": "rm -rf node_modules yarn.lock && yarn",
    "test": "NODE_ENV=test jest",
    "watch": "tsc -w -p ./src"
  },
  "typings": "index",
  "dependencies": {
    "@nlabs/react-native-utils": "^0.1.0",
    "@types/luxon": "^0.5.0",
    "lodash": "^4.17.5",
    "luxon": "^0.5.3",
    "react-motion": "^0.5.2",
    "react-native-vector-icons": "^4.5.0"
  },
  "devDependencies": {
    "@types/babel-core": "^6.25.3",
    "@types/jest": "^22.1.4",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.16",
    "arkhamjs": "^3.2.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^4.0.0",
    "jest": "^22.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-native": "^0.53.3",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2"
  },
  "peerDependencies": {
    "arkhamjs": ">=3.2",
    "prop-types": ">=15.5",
    "react": ">=16.0",
    "react-native": ">=0.51.0"
  },
  "jest": {
    "automock": false,
    "bail": false,
    "browser": false,
    "globals": {
      "__DEV__": true
    },
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "android",
        "ios"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp|ttf|otf)$": "RelativeImageStub",
      "^React$": "<rootDir>/node_modules/react"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/react-native/Libraries/react-native/",
      "<rootDir>/node_modules/react-native/packager/"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*test.ts?(x)"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/?!(react-native)"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "<rootDir>/node_modules/react-native/"
    ],
    "verbose": false
  }
}